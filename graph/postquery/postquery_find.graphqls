extend type PostQuery {
    find(
        filter:PostFilter
    ):PostDataResult! @goField(forceResolver: true)
}

input PostFilter{
    limit: Int!
    offset: Int!
    data: PostIDHashtagData! @inputUnion

}
input PostIDHashtagData {
    hashtags: String
    id:[String]
}

type PostFindOK {
    posts:[Post]
}

union PostDataResult = InternalErrorProblem | PostFindOK
